								<xsd:keyref name="FK_ProteinAmountRatiosConditionDenominator" refer="PK_CONDITION">
												<xsd:selector xpath="project/ratios/protein_amount_ratios/excel_ratio/denominator"/>
												<xsd:field xpath="@conditionRef"/>
