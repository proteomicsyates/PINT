												<xsd:field xpath="@conditionRef"/>
								</xsd:keyref>
								<xsd:keyref name="FK_QuantificationProteinRatiosNumeratorConditionRef" refer="PK_CONDITION">
												<xsd:selector xpath="project/ratios/protein_amount_ratios/remoteFiles_ratio/numerator"/>
